var SITE = {
    init: function() {
        console.log("Hotels DC Summersale Bell Boy | author: YiCOSA yozenci@gmail.com");

        var myElement = document.getElementById("border");

        myElement.addEventListener("click", function() {
            // Enabler.exit('Background Exit');
            window.open(clickTag);
        });
        SITE.loadLottie2(); 
        // Call the animate function
        TweenLite.delayedCall(0, SITE.animate);

    },
loadLottie: function() {
        SITE.lottieAnim = lottie.loadAnimation({
            container: document.getElementById('lottie-container'),
            renderer: 'svg',
            loop: true,
            autoplay: false,
            animationData: jsonAnim // Update with your JSON path
        });
    },

    loadLottie2: function() {
        SITE.lottieAnim2 = lottie.loadAnimation({
            container: document.getElementById('lottie-container-2'),
            renderer: 'svg',
            loop: false,
            autoplay: false,
            animationData: jsonAnim2 // Update with your JSON path
        });
    },

    playLottie: function() {

        if (SITE.lottieAnim) {
            const lottieContainer = document.getElementById("lottie-container");
            lottieContainer.style.opacity = "1";
            SITE.lottieAnim.goToAndPlay(0, true);
        }
    },

    playLottie2: function() {

        if (SITE.lottieAnim2) {
            const lottieContainer2 = document.getElementById("lottie-container-2");
            lottieContainer2.style.opacity = "1";
            SITE.lottieAnim2.goToAndPlay(0, true);
        }
    },     
    maskOut: function() {
        const masktl = new TimelineMax({repeat: 0, repeatDelay: 2});
        masktl.to(["#mask"], 0.3, {autoAlpha: 0, ease: Power1.easeIn}, "+=0.2")
        ;
    }, 
    maskIn: function() {
        const masktl = new TimelineMax({repeat: 0, repeatDelay: 2});
        masktl.to(["#mask"], 0.3, {autoAlpha: 1, ease: Power1.easeIn}, "+=0")
        ;
    },
    imgMaskIn: function() {
        const imgmasktl = new TimelineMax({repeat: 0, repeatDelay: 2});
        imgmasktl.to(["#img_mask"], 0.1, {autoAlpha: 1}, "+=0")
        ;
    }, 
    imgMaskOut: function() {
        const imgmasktl = new TimelineMax({repeat: 0, repeatDelay: 2});
        imgmasktl.to(["#img_mask"], 0.1, {autoAlpha: 0}, "+=0")
        ;
    },        
    animate: function() {

        //cta animation
        const container = document.getElementById('border');
        const cta3 = document.getElementById('cta3');
        const cta4 = document.getElementById('cta4');

        // Pulse settings
        const pulseSpeed = 0.25;
        const pulseScale = 1.15;
        const pulseScale2 = 1.05;        

        // Initial element position settings
        TweenLite.set("#card", { x: -80 });
        TweenLite.set("#mask", {x: -80 });

        TweenLite.set("#bellboy2", {y: 25 });
        TweenLite.set("#bellboy3", {y: -14});
        TweenLite.set(".c1", { y: -6 });
        
        // const shakeDur = 0.04;
        // const shakeValue = 3;
        const copyDur = 0.4;
        const copyDel = "+=0";
        const copyEase = Bounce.easeOut; 

        // Create a GSAP timeline
        const timeline = new TimelineMax({repeat: 0, repeatDelay: 2});

        // Set #container to be visible
        timeline.to(["#container"], 0, {autoAlpha: 1}, "+=0")
        .to(["#card","#mask"], 0.64, {x:584, ease: Power1.easeOut,onStart:SITE.maskOut},"+=0.9")
        .to(["#logo1"], 0.2, {autoAlpha: 0, ease: Power1.easeIn}, "-=0.64")

        .to("#c11", 0, { autoAlpha: 1 }, "+=0") //Members Save
        .to("#c11", copyDur, { y: 0, ease: copyEase})
        .to("#c11", 0, { autoAlpha: 0 }, "+=1.1") 

        .to("#c12", 0, { autoAlpha: 1 }, copyDel) //Up to 20%
        .to("#c12", copyDur, { y: 0, ease: copyEase}, copyDel)
        .to("#c12", 0, { autoAlpha: 0 }, "+=1.1") 

        .to("#c13", 0, { autoAlpha: 1 }, copyDel) //On Hotels
        .to("#c13", copyDur, { y: 0, ease: copyEase}, copyDel)
        .to("#c13", 0, { autoAlpha: 0 }, "+=1.1") 

        .to("#bellboy2", 0, { autoAlpha: 1 }, "+=0.1")
        .to(["#bellboy2"], 0.3, {y:0, autoAlpha:1,ease: Power1.easeOut})
        .to(["#speechline1","#c2"], 0.3, {autoAlpha:1, ease: Power1.easeOut },"-=0.3")

        .to(["#bellboy2"], 0.2, {y:-14, autoAlpha:1,ease: Power1.easeOut},"+=1.5")
        .to(["#bellboy2"], 0, {autoAlpha:0})
        .to(["#bellboy3"], 0, {autoAlpha:1})
        .to(["#bellboy3"], 0.2, {y:0, autoAlpha:1,ease: Power1.easeIn})

        .to(["#c2","#speechline1"], 0, {autoAlpha:0 },"-=0.2")
        .to(["#c3","#speechline2"], 0, {autoAlpha:1 },"+=0")

        .to(["#speechline2","#c3"], 0.2, {autoAlpha:1, ease: Power1.easeOut },"+=0")

        // .to(["#card","#mask"], 0.55, {x:-145, ease: Power1.easeIn, onStart:SITE.maskIn},"+=1.7")
        .to(["#card","#mask"], 0.55, {x:-145, ease: Power1.easeIn, onStart:SITE.maskIn},"+=2.3")
        .to(["#logo2"], 0.6, {autoAlpha:1, ease: Power1.easeOut},"-=0.2")
        .to(["#tac"], 0, {autoAlpha:1, ease: Power1.easeOut, onStart:SITE.playLottie2 },"-=0.3")
        ;
        
        timeline.play();
        // document.getElementById('guide').style.backgroundImage = "url('1.png')";
        // timeline.pause(0);
        console.log(document.title, "Animation length: ", timeline.duration(),"Total length: ", (timeline.repeat() + 1)*timeline.duration());

    }
};

// Initialize the site
SITE.init();
